        <activity android:label="@string/app_name" android:name=".college.collegedata"  />

        <activity android:label="@string/app_name" android:name=".hostel.hostelarea"  />
        <activity android:label="@string/app_name" android:name=".hostel.hostellist"  />
        <activity android:label="@string/app_name" android:name=".hostel.hosteldata"  android:parentActivityName=".hostel.hostellist"/>

        <activity android:label="@string/app_name" android:name=".hotel.hotelarea"  />
        <activity android:label="@string/app_name" android:name=".hotel.hotellist"  />
        <activity android:label="@string/app_name" android:name=".hotel.hoteldata"  android:parentActivityName=".hotel.hotellist"/>

        <activity android:label="@string/app_name" android:name=".train.traintype"/>
        <activity android:label="@string/app_name" android:name=".train.trainbetweenstations"/>
        <activity android:label="@string/app_name" android:name=".train.trainlist"/>
        <activity android:label="@string/app_name" android:name=".train.traintimings"/>

        <activity android:label="@string/app_name" android:name=".emergencynos.emergencyno"/>

        <activity android:label="@string/app_name" android:name=".shopping"/>
